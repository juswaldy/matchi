<h2>Previous games:</h2>
<table>
  <tr>
    <th>Date</th>
    <th>Team1</th>
    <th>vs</th>
    <th>Team2</th>
    <th></th>
    <th></th>
  </tr>

<% @games.each do |game| %>
  <tr class="<%= cycle('item_odd', 'item_even') %>">
    <td><%= game.date %></td>
    <td><%= Team.find(game.team1id).name + " (#{game.team1score})" %></td>
    <td></td>
    <td><%= Team.find(game.team2id).name + " (#{game.team2score})" %></td>
    <td><%= link_to 'Show', game %></td>
    <td><%= link_to 'Edit', edit_game_path(game) %></td>
  </tr>
<% end %>
</table>
<br /><br />
<div>
>>>>>>>>>>> <%= link_to 'Create a new game', '/welcome/availability' %>
</div>
